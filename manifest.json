{
    "manifest_version": 3,
    "name": "TextSearch",
    "description": "Select text and easily search about it in google.",
    "version": "2.0.0",
    "icons": {
        "128": "icon.png"
    },
    "action": {
        "default_icon": {
            "32": "icon.png"
        }
    },
    "permissions": ["contextMenus"],
    "background": {
        "service_worker": "background.js"
    }
}